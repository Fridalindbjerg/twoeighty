<style>
  h1 {
    margin-bottom: 1rem;
  }
  .centered {
    text-align: center;
    margin-top: 1rem;
    margin-bottom: 4rem;
  }

  .centered1 {
    text-align: center;
    margin-bottom: 100px;
  }
  .margin_b {
    margin-bottom: 100px;
  }
  .aspect {
    height: 100%;
    object-fit: cover;
  }
  .flex_row {
    display: flex;
    flex-direction: row;
  }

  .span2 {
    grid-column: span 2;
  }

  .span3 {
    grid-column: span 3;
  }
  .span4 {
    grid-column: span 4;
  }
  h1 {
    margin-top: 6rem;
  }
  h3 {
    margin-bottom: 50px;
    text-align: center;
  }

  img {
    max-width: 100%;
  }

  .gap {
    display: flex;
    gap: 25px;
  }

  section {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 10px;
  }

  @media (width >= 600px) {
    h1 {
      margin-bottom: 3rem;
    }
    .centered {
      display: none;
    }

    section {
      gap: 25px;
    }
  }
</style>
